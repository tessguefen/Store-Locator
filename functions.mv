		<MvCLOSEVIEW NAME = "Merchant" VIEW = "TGSL_AddlFields">
		<MvFUNCTIONRETURN VALUE = "{ [ g.Module_Library_DB ].Error_Load_EOF( 'TGSL_Locations-LOCATION-0013' ) }">
	</MvIF>

	<MvEVAL EXPR = "{ AdditionalField_Read( l.field ) }">
